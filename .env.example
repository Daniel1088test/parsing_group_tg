# Django settings
DEBUG=False
SECRET_KEY=your_django_secret_key
ALLOWED_HOSTS=.railway.app,your-app-name.up.railway.app

# Database settings - Railway automatically provides DATABASE_URL
DATABASE_URL=postgresql://postgres:password@localhost:5432/railway

# Telegram Bot settings
BOT_TOKEN=your_telegram_bot_token

# Telethon settings (for user account)
API_ID=your_telegram_api_id
API_HASH=your_telegram_api_hash
SESSION_NAME=telethon_session

# Web server settings
WEB_SERVER_HOST=0.0.0.0
WEB_SERVER_PORT=${PORT}

# Railway specific
PORT=8000 